{
  "name": "chunked-upload",
  "version": "0.0.0",
  "description": "Chunked uploader for web apps. Uploads a file as a series of 'chunks' for increased resiliency.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack",
    "dev": "webpack-dev-server --port 3000 --hot --inline --colors --history-api-fallback",
    "dev:quiet": "webpack-dev-server --port 3000 --hot --inline --colors --no-info --history-api-fallback --quiet",
    "test": "npm run test:unit",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "deploy": "npm run test && npm run compile"
  },
  "author": "Ben Lerchin <ben@lerchin.com> (http://benlerchin.com)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.2.15",
    "babel-core": "^5.2.15",
    "babel-eslint": "^5.0.4",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.4.7",
    "css-loader": "^0.12.0",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "extract-text-webpack-plugin": "^0.8.0",
    "html-webpack-plugin": "^1.3.0",
    "node-sass": "^3.2.0",
    "object-assign": "^3.0.0",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.1",
    "webpack": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "event-emitter": "^0.3.4",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.5.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^0.3.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.6.1",
    "mocha": "^2.2.5",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "shortid": "^2.2.6",
    "sinon": "^2.0.0-pre.2",
    "superagent": "^2.1.0",
    "webpack-dev-server": "^1.8.2"
  }
}
